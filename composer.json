{
    "name": "monicahq/laravel-sabre",
    "description": "Sabre DAV server adapter for Laravel.",
    "keywords": [
        "laravel",
        "php",
        "sabre",
        "dav"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/monicahq/laravel-sabre/issues",
        "source": "https://github.com/monicahq/laravel-sabre"
    },
    "authors": [
        {
            "name": "Alexis Saettler",
            "email": "alexis@saettler.org"
        }
    ],
    "require": {
        "laravel/framework": "^5.6 || ^6.0",
        "sabre/dav": "^4.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.3.15 || ^0.5.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^8.0",
        "vimeo/psalm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelSabre\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSabre\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSabre\\LaravelSabreServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
