{
    "name": "monicahq/laravel-sabre",
    "description": "Sabre DAV server adapter for Laravel.",
    "keywords": [
        "laravel",
        "php",
        "sabre",
        "dav"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/monicahq/laravel-sabre/issues",
        "source": "https://github.com/monicahq/laravel-sabre"
    },
    "authors": [
        {
            "name": "Alexis Saettler",
            "email": "alexis@saettler.org"
        }
    ],
    "require": {
        "illuminate/support": "^11.0 || ^12.0",
        "sabre/dav": "^4.0",
        "thecodingmachine/safe": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.0 || ^10.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.0 || ^12.0",
        "roave/security-advisories": "dev-master",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "vimeo/psalm": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelSabre\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSabre\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSabre\\LaravelSabreServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ocramius/package-versions": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
